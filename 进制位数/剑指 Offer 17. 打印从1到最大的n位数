class Solution {
public:
    vector<int> printNumbers(int n) {
        vector<int> ans;
        int len=10;
        for(int i=1;i<n;++i){                 //或者用10^n-1，这里可能要考虑下大数溢出的情况
            len=len*10;
        }
        for(int i=1;i<len;++i){
            ans.push_back(i);
        }
        return ans;
    }
};
